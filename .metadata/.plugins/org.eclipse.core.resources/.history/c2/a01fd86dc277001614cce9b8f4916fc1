package org.conxworks.masterdata.dao.api.organization;

import org.conxworks.core.api.ApplicationException;
import org.conxworks.core.dao.api.IBaseDAO;
import org.conxworks.masterdata.model.organization.Organization;


public interface IOrganizationDAOService extends IBaseDAO<Organization> {
	public Organization provide(Organization record) throws ApplicationException;

	public Organization getByCode(String code) throws ApplicationException;

	public Organization getByName(String name) throws ApplicationException;
}
